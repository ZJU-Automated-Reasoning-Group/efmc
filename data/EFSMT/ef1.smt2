(declare-fun x1 () (_ BitVec 64))
(declare-fun x2 () (_ BitVec 64))
(declare-fun x3 () (_ BitVec 64))
(declare-fun x4 () (_ BitVec 64))
(declare-fun x5 () (_ BitVec 64))
(declare-fun x6 () (_ BitVec 64))
(declare-fun x7 () (_ BitVec 64))
(assert
    (forall ((y1 (_ BitVec 64)) (y2 (_ BitVec 64)) (y3 (_ BitVec 64)) 
             (y4 (_ BitVec 64)) (y5 (_ BitVec 64)))
        (=> (and (bvult y1 #x0000000000000FFF)
                 (bvugt y2 #x0000000000000800)
                 (bvslt y3 #x0000000000000100)
                 (bvsge y4 #x0000000000000000)
                 (= y5 (bvadd (bvmul y1 y2) (bvudiv y3 y4))))
            (or (and (= (bvmul x1 y1) (bvadd x2 y2))
                    (bvule (bvand x2 y3) (bvor x3 y4))
                    (= (bvshl x3 y5) x4)
                    (bvugt (bvlshr x4 y1) x5))
                (and (= (bvashr x5 y2) x6)
                     (= (bvxor x6 y3) x7)
                     (bvult (bvnot x7) y4)
                     (= (bvneg x1) y5))))))
(check-sat)