(set-logic HORN)
(declare-fun inv ((_ BitVec 64) (_ BitVec 64) (_ BitVec 64)) Bool)
(assert (forall ((x (_ BitVec 64)) (y (_ BitVec 64)) (z (_ BitVec 64)))
  (=> (and (= x #x0000000000000000) (= y z) (= z #x0000000000000000))
      (inv x y z))))
(assert (forall ((x (_ BitVec 64))
         (y (_ BitVec 64))
         (z (_ BitVec 64))
         (x! (_ BitVec 64))
         (y! (_ BitVec 64))
         (z! (_ BitVec 64)))
  (let ((a!1 (and (inv x y z)
                  (= x! (bvadd x #x0000000000000001))
                  (= y!
                     (ite (bvuge x #x00000000000006e5)
                          (bvadd y #x0000000000000002)
                          (bvadd y #x0000000000000001)))
                  (= z!
                     (ite (bvuge y #x0000000000001685)
                          (bvadd z #x0000000000000003)
                          (bvadd z #x0000000000000002))))))
    (=> a!1 (inv x! y! z!)))))
(assert (forall ((x (_ BitVec 64)) (y (_ BitVec 64)) (z (_ BitVec 64)))
  (let ((a!1 (not (and (not (bvule x #x00000000000044f2))
                       (bvule z #x0000000000006c02)))))
    (=> (inv x y z) a!1))))
(check-sat)
