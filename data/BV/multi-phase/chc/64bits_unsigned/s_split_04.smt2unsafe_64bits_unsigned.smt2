(set-logic HORN)
(declare-fun inv ((_ BitVec 64) (_ BitVec 64) (_ BitVec 64)) Bool)
(assert (forall ((y (_ BitVec 64)) (x (_ BitVec 64)) (z (_ BitVec 64)))
  (=> (and (= x #x0000000000000000)
           (= y #x0000000000000000)
           (= z #x0000000000000000))
      (inv y x z))))
(assert (forall ((y (_ BitVec 64))
         (x (_ BitVec 64))
         (z (_ BitVec 64))
         (x! (_ BitVec 64))
         (y! (_ BitVec 64))
         (z! (_ BitVec 64)))
  (let ((a!1 (and (inv y x z)
                  (= x! (bvadd x #x0000000000000001))
                  (= y! (bvadd y x))
                  (= z! (ite (bvugt y x) (bvadd z #x0000000000000001) z)))))
    (=> a!1 (inv x! y! z!)))))
(assert (forall ((y (_ BitVec 64)) (x (_ BitVec 64)) (z (_ BitVec 64)))
  (let ((a!1 (not (and (bvuge x #x000000000034adcd)
                       (not (bvule z #x0000000000000000))))))
    (=> (inv y x z) a!1))))
(check-sat)
