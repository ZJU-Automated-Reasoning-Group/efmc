(set-logic HORN)
(declare-fun inv ((_ BitVec 32) (_ BitVec 32)) Bool)
(assert (forall ((x (_ BitVec 32)) (z (_ BitVec 32)))
  (=> (and (= x #x00000000) (= z #x00000000)) (inv x z))))
(assert (forall ((x (_ BitVec 32))
         (z (_ BitVec 32))
         (x! (_ BitVec 32))
         (z! (_ BitVec 32)))
  (let ((a!1 (= x!
                (ite (bvult (bvmul x #x00000005) z)
                     (bvadd x #x00000001)
                     (bvudiv x #x0000000a))))
        (a!2 (= z! (ite (bvult (bvmul x #x00000005) z) z (bvadd #x00000001 z)))))
    (=> (and (inv x z) a!1 a!2) (inv x! z!)))))
(assert (forall ((x (_ BitVec 32)) (z (_ BitVec 32)))
  (let ((a!1 (not (and (not (bvule z #x00000032)) (bvule z x)))))
    (=> (inv x z) a!1))))
(check-sat)
