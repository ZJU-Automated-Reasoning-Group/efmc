(set-logic HORN)
(declare-fun inv ((_ BitVec 64) (_ BitVec 64)) Bool)
(assert (forall ((x (_ BitVec 64)) (y (_ BitVec 64)))
  (=> (and (= x #x000000000000c350) (= y #x0000000000000000)) (inv x y))))
(assert (forall ((x (_ BitVec 64))
         (y (_ BitVec 64))
         (x! (_ BitVec 64))
         (y! (_ BitVec 64)))
  (let ((a!1 (and (inv x y)
                  (= x! (ite (bvsge y x) (bvadd x #x0000000000000005) x))
                  (= y! (ite (bvsge y x) y (bvadd y #x0000000000000001))))))
    (=> a!1 (inv x! y!)))))
(assert (forall ((x (_ BitVec 64)) (y (_ BitVec 64)))
  (let ((a!1 (bvadd x
                    (bvmul (bvadd #x0000000000000001 (bvnot #x0000000000000001))
                           y))))
  (let ((a!2 (not (and (not (bvsle y #x000000000000c350))
                       (not (bvsle a!1 #x0000000000000005))))))
    (=> (inv x y) a!2)))))
(check-sat)
