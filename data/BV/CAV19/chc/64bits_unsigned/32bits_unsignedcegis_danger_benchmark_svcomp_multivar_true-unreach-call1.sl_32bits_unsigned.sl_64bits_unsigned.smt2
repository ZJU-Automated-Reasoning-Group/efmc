(set-logic HORN)
(declare-fun inv ((_ BitVec 64) (_ BitVec 64) ) Bool)
(assert (forall ((x (_ BitVec 64)) (y (_ BitVec 64))) 
       (=> ( = x y ) (inv x y))))
(assert (forall ((x (_ BitVec 64)) (x! (_ BitVec 64)) (y (_ BitVec 64)) (y! (_ BitVec 64))) 
       (=> (and (inv x x!) ( and ( bvult x #x00000400 ) ( = x! ( bvadd x #x00000001 ) ) ( = y! ( bvadd y #x00000001 ) ) )) (inv y y!))))
(assert (forall ((x (_ BitVec 64)) (y (_ BitVec 64))) 
       (=> (inv x y) ( or ( = x y ) ( bvult x #x00000400 ) ))))
