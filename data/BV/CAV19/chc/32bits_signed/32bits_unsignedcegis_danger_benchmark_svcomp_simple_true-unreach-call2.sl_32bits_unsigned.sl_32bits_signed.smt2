(set-logic HORN)
(declare-fun inv ((_ BitVec 32) ) Bool)
(assert (forall ((x (_ BitVec 32))) 
       (=> ( = x x ) (inv x))))
(assert (forall ((x (_ BitVec 32)) (x! (_ BitVec 32))) 
       (=> (and (inv x) ( and ( bvslt x #x0fffffff ) ( = x! ( bvadd x #x00000001 ) ) )) (inv x!))))
(assert (forall ((x (_ BitVec 32))) 
       (=> (inv x) ( or ( bvsge x #x0fffffff ) ( bvslt x #x0fffffff ) ))))
(check-sat)
